    <string name="intro_new_wallet_continue_message">Please confirm you have properly stored your Wallet Seed somewhere safe.</string>
    <string name="intro_new_wallet_continue_positive">I have backed up my Wallet Seed</string>
    <string name="intro_new_wallet_continue_negative">Back</string>
