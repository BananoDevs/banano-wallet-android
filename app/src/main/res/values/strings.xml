    <string name="intro_new_wallet_title">Your Nano Wallet Seed</string>
    <string name="intro_new_wallet_message"><![CDATA[<font color=\"#000034\">Your Nano Wallet Seed is how we generate your wallet. It\'s also how you log into the Android Wallet or any of our other wallets, including NanoWallet.io</font><br><br><font color=\"#ee4b66\">If you lose your Wallet Seed, your funds cannot be recovered.</font><br><br><font color=\"#000034\">Keep your Wallet Seed somewhere safe (like 1Password, LastPass, or print it out and put it in a safe) and </font><font color=\"#ee4b66\">never give it to anyone, ever.</font>]]></string>
    <string name="intro_new_wallet_confirm">I Understand, Continue</string>
    <string name="intro_new_wallet_seed_copied">Your Wallet Seed is pastable for 2 minutes. After, you can access it in Settings.\n\nPlease backup your Wallet Seed somewhere safe like 1Password, LastPass, or print it out and put it in a safe.</string>
    <string name="intro_new_wallet_seed_done">Okay</string>
